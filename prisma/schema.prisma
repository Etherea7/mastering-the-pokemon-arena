generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./pokemon_data.db?connection_limit=1&connect_timeout=30&pool_timeout=30&socket_timeout=30&mode=readonly"
}

model PokemonAbilities {
  name          String?
  generation    String?
  battle_format String?
  rating        Int?
  year_month    String?
  Ability       String?
  Usage         Float?
  id            String? @default(cuid())

  @@unique([name, generation, battle_format, rating, year_month, Ability], map: "unique_pokemon_abilities")
  @@index([Usage], map: "idx_pokemon_abilities_usage")
  @@index([Ability], map: "idx_pokemon_abilities_ability")
  @@map("pokemon_abilities")
  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model PokemonBase {
  name              String?
  raw_count         Int?
  avg_weight        Float?
  viability_ceiling Int?
  generation        String?
  battle_format     String?
  rating            Int?
  year_month        String?
  id                String? @default(cuid())

  @@unique([name, generation, battle_format, rating, year_month], map: "unique_pokemon_base")
  @@index([viability_ceiling], map: "idx_base_viability")
  @@index([generation, battle_format], map: "idx_base_gen_format")
  @@index([name], map: "idx_base_name")
  @@map("pokemon_base")
  @@ignore
}

